{
  "name": "county-committee",
  "description": "App for county committee information",
  "version": "0.0.0",
  "homepage": "https://ccsunlight.org",
  "main": "src/",
  "keywords": ["feathers"],
  "license": "MIT",
  "repository": {},
  "author": {
    "name": "Jonathan Crockett & Dave Druelinger"
  },
  "contributors": [],
  "bugs": {},
  "engines": {
    "node": ">= 6.10.2"
  },
  "scripts": {
    "install-deps": "apt-get -y update && apt-get -y install poppler-utils",
    "dev": "nodemon  --inspect=0.0.0.0 -e js,json,hbs src/",
    "start": "node --inspect=0.0.0.0 src/",
    "build-admin": "cd public/cc-admin && npm run build",
    "cert": "node cert/gene rate-cert.js",
    "test:watch":
      "nodemon  --inspect=0.0.0.0 --exec 'mocha --recursive --bail test/services/**/*.test.js'",
    "precommit": "lint-staged",
    "format": "prettier",
    "jshint": "jshint src/. test/. --config",
    "import-certified-list": "node scripts/import-certified-list",
    "prettier-watch":
      "onchange '**/*.{js,json,css,md}' -- prettier --write {{changed}}",
    "sync-db": "bash scripts/sync-db-from-prod.sh",
    "migrate": "bash ./init-migration.sh"
  },
  "nodemonConfig": {
    "ext": "js",
    "ignore": [],
    "events": {
      "start": "echo starting NODEMON"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": ["prettier --write", "git add"]
  },

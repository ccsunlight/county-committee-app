{
  "name": "county-committee",
  "description": "App for county committee information",
  "version": "2.0.0",
  "homepage": "https://ccsunlight.org",
  "main": "src/",
  "keywords": [
    "feathers"
  ],
  "license": "MIT",
  "repository": {},
  "author": {
    "name": "Jonathan Crockett & Dave Druelinger"
  },
  "contributors": [],
  "bugs": {},
  "engines": {
    "node": ">= 6.10.2"
  },
  "scripts": {
    "preinstall": "bash ./pre-install.sh",
    "postinstall": "bash ./post-install.sh",
    "dev": "nodemon  --inspect=0.0.0.0 -e js,json,hbs src/",
    "start": "node --inspect=0.0.0.0 src/",
    "build-admin": "cd public/cc-admin && npm install --no-bin-links && npm install -g -Y react-scripts  && npm run build",
    "migrate": "bash ./init-migration.sh",
    "install-pdf-lib": "apt-get -y update && apt-get -y install poppler-utils",
    "cert": "node cert/gene rate-cert.js",
    "test:watch": "nodemon  --inspect=0.0.0.0 --exec 'mocha --recursive --bail test/services/**/*.test.js'",
    "test": "mocha  --inspect=0.0.0.0 --recursive --exit --bail test/services/**/*.test.js",
    "precommit": "lint-staged",
    "format": "prettier",
    "jshint": "jshint src/. test/. --config",
    "prettier-watch": "onchange '**/*.{js,json,css,md}' -- prettier --write {{changed}}"
  },
  "nodemonConfig": {
    "ext": "js",
    "ignore": [],
    "events": {
      "start": "echo starting NODEMON"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],

<!doctype html>
<html lang="en">
{{> head}}

<body class="mdl-demo mdl-color--grey-100 mdl-color-text--grey-700 mdl-base">
	<div class="mdl-layout mdl-js-layout mdl-layout--fixed-header">
		{{> header}}

		<main class="mdl-layout__content">

			{{{body}}}

			{{> footer}}

		</main>
	</div>

	<script>
		// Only load JS for touch devices
		if ('ontouchstart' in document.documentElement) {
			(function () {
				var script = document.createElement('script');
				script.type = 'text/javascript';
				script.src = 'https://code.getmdl.io/1.3.0/material.min.js';
				script.async = true;
				document.body.appendChild(script);
				console.log('loadScript');
			})();

		}
	</script>
	<script src="https://cdn.jsdelivr.net/lodash/4.17.4/lodash.core.min.js"></script>


	<script defer
		src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDllCUiwagZYbAgEG92cGnYYdMjwx7_XvY&libraries=places&callback=initGoogleApis"></script>
	<script>
		(function (i, s, o, g, r, a, m) {
			i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
				(i[r].q = i[r].q || []).push(arguments)
			}, i[r].l = 1 * new Date(); a = s.createElement(o),
				m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
		})(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');
		ga('create', 'UA-102435046-1', 'auto');
		ga('send', 'pageview');
	</script>
</body>

</html>